{
    "name": "react-accessible-shuttle",
    "version": "0.2.3",
    "description": "A tiny, zero dependency, shuttle (a.k.a list shuttle, dual listbox, etc.) implementation in React using hooks.",
    "main": "index.js",
    "scripts": {
        "test": "jest --silent",
        "build": "npm run clean && npm run build:css && pika build && npm run clean:css",
        "build:css": "sass -s compressed --embed-source-map src/styles/** ./css/shuttle.css",
        "build:test": "npm run clean && tsc src/index.ts --pretty --jsx react --esModuleInterop  --downLevelIteration --outDir pkg",
        "clean": "rimraf pkg/**",
        "clean:css": "rimraf ./css",
        "version": "npm run build"
    },
    "@pika/pack": {
        "pipeline": [
            [
                "@pika/plugin-ts-standard-pkg",
                {
                    "excludes": "playground/**"
                }
            ],
            [
                "@pika/plugin-build-web"
            ],
            [
                "@pika/plugin-build-node"
            ],
            [
                "pika-plugin-legacy-browser",
                {
                    "name": "ReactShuttle",
                    "minify": true,
                    "external": [
                        "react"
                    ],
                    "globals": {
                        "react": "React"
                    },
                    "sourcemap": true
                }
            ],
            [
                "@pika/plugin-copy-assets",
                {
                    "files": [
                        "css/"
                    ]
                }
            ]
        ]
    },
    "keywords": [
        "shuttle",
        "dual listbox",
        "list shuttle",
        "hooks"
    ],
    "author": "Dennis Thompson",
    "maintainers": [
        "Dennis Thompson"
    ],
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
        "@babel/plugin-transform-destructuring": "^7.5.0",
        "@babel/plugin-transform-object-assign": "^7.2.0",
        "@babel/preset-react": "^7.0.0",
        "@djthoms/eslint-config": "^1.1.0",
        "@djthoms/prettier-config": "^1.0.0",
        "@pika/pack": "^0.5.0",
        "@pika/plugin-build-node": "^0.6.0",
        "@pika/plugin-build-umd": "^0.6.0",
        "@pika/plugin-build-web": "^0.6.0",
        "@pika/plugin-copy-assets": "^0.6.0",
        "@pika/plugin-ts-standard-pkg": "^0.6.0",
        "@testing-library/jest-dom": "^4.0.0",
        "@testing-library/react": "^9.1.1",
        "@testing-library/react-hooks": "^2.0.1",
        "@types/classnames": "^2.2.9",
        "@types/jest": "^24.0.17",
        "@types/react": "^16.9.1",
        "@types/react-dom": "^16.8.5",
        "@typescript-eslint/parser": "^2.0.0",
        "concurrently": "^4.1.1",
        "core-js": "^3.2.1",
        "coveralls": "^3.0.6",
        "eslint": "^5.16.0",
        "flowgen": "^1.10.0",
        "jest": "^24.8.0",
        "mini-css-extract-plugin": "^0.8.0",
        "mkdirp": "^0.5.1",
        "pika-plugin-legacy-browser": "^1.1.1",
        "prettier": "^1.18.2",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-test-renderer": "^16.9.0",
        "react-window": "^1.8.5",
        "rimraf": "^3.0.0",
        "sass": "^1.22.9",
        "ts-jest": "^24.0.2",
        "typescript": "^3.5.3"
    },
    "dependencies": {
        "classnames": "^2.2.6"
    }
}
